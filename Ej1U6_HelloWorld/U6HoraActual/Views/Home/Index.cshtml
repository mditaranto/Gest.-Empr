@{
    ViewData["Title"] = "Home Page";

    DateTime currentDateTime = DateTime.Now;
    String formattedDateTime = currentDateTime.ToString("HH:mm");
    int Hora = currentDateTime.Hour;
    String medio = "";
    if (Hora > 12)
    {
        medio = "Son mas de las 12";
    } else
    {
        medio = "Son menos de las 12";
    }

  
}

<asp:Label ID="Label1" runat="server" Text="Label">Son las @formattedDateTime</asp:Label>
<label>@medio</label>

<table border="1">
        @for(var i = 1; i <= 10; i++)
    {
        <tr>
            @for(var a = 1; a <= 10; a++)
            {
                <td class="border-1">
                @i * @a = @(i*a)</td>
            }
        </tr>
    }
</table>